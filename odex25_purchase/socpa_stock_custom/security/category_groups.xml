<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Category -->
    <record model="ir.module.category" id="stock_inventory">
        <field name="name">Stock Inventory Committee</field>
        <field name="description">User access level for this module</field>
        <field name="sequence">50</field>
    </record>
    

    <record id="inventory_committee_member" model="res.groups">
        <field name="name">Member</field>
        <field name="category_id" ref="stock_inventory"/>
    </record>



    <record id="inventory_committee_head" model="res.groups">
        <field name="name">Head</field>
        <field name="category_id" ref="stock_inventory"/>
    </record>


    <record id="pruduct_quants_config" model="res.groups">
        <field name="name">Product gift Quants Config</field>
        <field name="category_id" ref="exchange_request.exchange_request_custom"/>
    </record>


    <record id="supply_request" model="res.groups">
        <field name="name">Create Supply Request</field>
        <field name="category_id" ref="exchange_request.exchange_request_custom"/>
    </record>
</odoo>
