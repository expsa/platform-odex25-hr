<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        
        <record id="purchase_order_approved_seq" model="ir.sequence">
            <field name="name">Purchase Order Approved Sequence</field>
            <field name="code">purchase.order.new_seq</field>
            <field name="prefix">PO</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">no_gap</field>
        </record>

        <record id="purchase.seq_purchase_order" model="ir.sequence">
            <field name="name">Purchase Order</field>
            <field name="code">purchase.order</field>
            <!-- <field name="prefix">P</field> -->
            <field name="prefix">RFQ</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        
    </data>
</openerp>
