<?xml version='1.0' encoding='utf-8'?>
<odoo>
   <!-- account.move inherit form view -->
   <record id="invoice_form_view" model="ir.ui.view">
       <field name="name">account.move.inherit.view.form</field>
       <field name="model">account.move</field>
       <field name="inherit_id" ref="account.view_move_form"/>
       <field name="arch" type="xml">
            <xpath expr="//button[last()]" position="before">
                <button name="action_vendor_eval" type="object" string="Evaluate"/>                
            </xpath>
       </field>
   </record>

   

    

   
</odoo>