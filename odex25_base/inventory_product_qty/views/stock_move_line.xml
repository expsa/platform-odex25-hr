<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_move_line_tree_warehouse_inheritt" model="ir.ui.view">
        <field name="name">stock.move.line.warehouse.inheritt</field>
        <field name="model">stock.move.line</field>
        <field name="inherit_id" ref="stock.view_move_line_tree" />
        <field name="arch" type="xml">

            <xpath expr="//tree" position="attributes">
                <attribute name="default_order">date desc</attribute>
            </xpath>

            <xpath expr="//field[@name='qty_done']" position="after">
                <field name="post_move_location_qty" />
            </xpath>

        </field>
    </record>

</odoo>