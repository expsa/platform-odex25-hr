<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="assets_backend" name="Mail Enterprise Assets" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/odex25_mail/static/src/js/attachment_viewer.js"/>
            <script type="text/javascript" src="/odex25_mail/static/src/bugfix/bugfix.js"/>
            <script type="text/javascript" src="/odex25_mail/static/src/widgets/form_renderer/form_renderer.js"/>

            <link rel="stylesheet" type="text/scss" href="/odex25_mail/static/src/scss/odex25_mail.scss"/>
            <link rel="stylesheet" type="text/scss" href="/odex25_mail/static/src/scss/odex25_mail_mobile.scss"/>
            <link rel="stylesheet" type="text/scss" href="/odex25_mail/static/src/bugfix/bugfix.scss"/>
            <link rel="stylesheet" type="text/scss" href="/odex25_mail/static/src/widgets/form_renderer/form_renderer.scss"/>
        </xpath>
    </template>

    <template id="assets_tests" name="Mail Enterprise Assets Tests" inherit_id="web.assets_tests">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/odex25_mail/static/tests/tours/mail_channel_expand_test_tour.js"/>
        </xpath>
    </template>

    <template id="qunit_suite" name="mail_tests" inherit_id="web.qunit_suite_tests">
        <xpath expr="." position="inside">
            <script type="text/javascript" src="/odex25_mail/static/src/bugfix/bugfix_tests.js"></script>
            <script type="text/javascript" src="/odex25_mail/static/tests/attachment_preview_tests.js"></script>
            <script type="text/javascript" src="/odex25_mail/static/src/widgets/form_renderer/form_renderer_tests.js"></script>
        </xpath>
    </template>

</odoo>
