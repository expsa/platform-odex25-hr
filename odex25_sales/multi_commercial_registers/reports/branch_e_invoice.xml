        <template id="branch_report_e_invoice" inherit_id="report_e_invoice.report_e_invoice"
                  customize_show="True">

            <xpath expr="//tr[@id='tr_company_report_name']" position="replace">
                <t t-if="o.print_by_branch">
                    <tr id="tr_branch_report_name">
                        <td class="info_left" id="td_company_report_name">
                            <t t-if="o.move_type in ['out_invoice','in_refund']">
                                <span t-field="o.branch_id.name"/>
                            </t>
                            <t t-if="o.move_type in ['out_invoice','in_refund']">
                                <span t-field="o.partner_id.name"/>
                            </t>
                        </td>
                        <td class="title" dir="rtl">
                            الاسم - Name
                        </td>
                        <td class="info_right">
                            <t t-if="o.move_type in ['out_invoice','in_refund']">
                                <span t-field="o.branch_id.name"/>
                            </t>
                            <t t-if="o.move_type in ['out_invoice','in_refund']">
                                <span t-field="o.partner_id.name"/>
                            </t>
                        </td>
                    </tr>
                </t>
                <t t-else="">
                    <tr id="tr_company_report_name">
                        <td class="info_left" id="td_company_report_name">
                            <t t-if="o.move_type in ['out_invoice','in_refund']">
                                <span t-field="o.company_id.partner_id.name"/>
                            </t>
                            <t t-if="o.move_type in ['out_invoice','in_refund']">
                                <span t-field="o.partner_id.name"/>
                            </t>
                        </td>
                        <td class="title" dir="rtl">
                            الاسم - Name
                        </td>
                        <td class="info_right">
                            <t t-if="o.move_type in ['out_invoice','in_refund']">
                                <span t-field="o.company_id.partner_id.name"/>
                            </t>
                            <t t-if="o.move_type in ['out_invoice','in_refund']">
                                <span t-field="o.partner_id.name"/>
                            </t>
                        </td>
                    </tr>
                </t>
            </xpath>

