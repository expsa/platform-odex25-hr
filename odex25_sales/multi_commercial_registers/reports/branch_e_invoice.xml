        <template id="branch_report_e_invoice_header" inherit_id="report_e_invoice.report_e_invoice_header" customize_show="True">
            <xpath expr="//div[@class='header']" position="inside">
                <t t-if="o.print_by_branch">
                    <div id="branch_div_header_logo" style="text-align: left;display: table-cell;">
                        <img t-if="o.branch_id.logo" t-att-src="'data:image/png;base64,%s' % to_text(o.branch_id.logo)"
                             style="max-height: 120px;" alt="Logo"/>
                    </div>
                </t>
                <t t-else="">
                    <div id="div_header_logo" style="text-align: left;display: table-cell;">
                        <img t-if="o.company_id.logo" t-att-src="'data:image/png;base64,%s' % to_text(o.company_id.logo)"
                             style="max-height: 120px;" alt="Logo"/>
                    </div>
                </t>
                <t t-if="o.print_by_branch">
                    <div id="branch_div_header_company_name" style="display: table-cell;text-align: right;">
                        <h3>
                            <t t-esc="o.branch_id.name"/>
                        </h3>
                    </div>
                </t>
                <t t-else="">
                    <div id="div_header_company_name" style="display: table-cell;text-align: right;">
                        <h3>
                            <t t-esc="o.company_id.name"/>
                        </h3>
                    </div>
                </t>
            </xpath>
        </template>


