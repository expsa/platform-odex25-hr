<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- Inherit Form View to Modify it -->
        <record id="view_pos_session_form_print_with_branch_inheritt" model="ir.ui.view">
            <field name="name">pos.session.print_with_branch.inheritt</field>
            <field name="model">pos.session</field>
            <field name="inherit_id" ref="point_of_sale.view_pos_session_form"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='branch_id']" position="after">
                    <field name="print_by_branch" readonly="1" force_save="1"/>
                </xpath>

            </field>
        </record>

                <!-- Inherit Form View to Modify it -->
        <record id="view_pos_pos_form_print_with_branch_inheritt" model="ir.ui.view">
            <field name="name">pos.order.print_with_branch.inheritt</field>
            <field name="model">pos.order</field>
            <field name="inherit_id" ref="point_of_sale.view_pos_pos_form"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='partner_id']" position="after">
                    <field name="print_by_branch" readonly="1" force_save="1"/>
                </xpath>

            </field>
        </record>



    </data>
</odoo>