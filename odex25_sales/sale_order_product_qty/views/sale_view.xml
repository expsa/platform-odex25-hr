<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <!-- Inherit Form View to Modify it -->
        <record id="view_order_form_qty_available_inheritt" model="ir.ui.view">
            <field name="name">sale.order.qty_available.inheritt</field>
            <field name="model">sale.order</field>
            <field name="inherit_id" ref="sale.view_order_form"/>
            <field name="arch" type="xml">

                <xpath expr="//field[@name='order_line']//tree//field[@name='product_id']" position="after">
                    <field name="qty_available" readonly="1" force_save="1"/>
                </xpath>
                <xpath expr="//field[@name='order_line']//form//field[@name='product_id']" position="after">
                    <field name="qty_available" readonly="1" force_save="1"/>
                    <field name="qty_warehouse" readonly="1" force_save="1"/>

                </xpath>

            </field>
        </record>

    </data>
</odoo>