                                    <tr class="text-center">
                                        <td>
                                            <span t-field="q.qualification_id.name"/>
                                        </td>
                                        <td>
                                            <span t-field="q.qualification_specification_id.name"/>
                                        </td>
                                        <td>
                                            <span t-field="q.uni_name"/>
                                        </td>
                                        <td>
                                            <span t-field="q.country_name"/>
                                        </td>
                                        <td>
                                            <span t-field="q.qualification_degree"/>
